<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="282.0" prefWidth="349.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.brh.fxstuff.fx_stuff.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label prefHeight="18.0" prefWidth="70.0" text="Volume">
         <VBox.margin>
            <Insets right="280.0" />
         </VBox.margin></Label>
      <Slider orientation="VERTICAL">
         <VBox.margin>
            <Insets right="280.0" />
         </VBox.margin></Slider>
      <Label fx:id="fileLabel" text="Datei:" />
      <Button mnemonicParsing="false" onAction="#onFile" text="File" />
      <Button mnemonicParsing="false" onAction="#onPlay" text="Play" />
      <Button mnemonicParsing="false" text="Stop" />
   </children>
</VBox>
